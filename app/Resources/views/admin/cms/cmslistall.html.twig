{% extends '::baseadmin.html.twig' %}


{% block body %}
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 ">
           <div class="addNew">
               <a href="{{ path('cmslist')}}">ADD Pages</a></div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="cmsFormList">
                <div class="header-title-wrapper">
                    <div class="header-title-sno">S.no</div>
                    <div class="header-title">Title</div>
                    <div class="header-title">Status</div>
                    <div class="header-title"></div>
                    <div class="header-title-action">
                        Action
                    </div>
                    <div class="clearboth"></div>
                </div>
                {% set i = 1 %}
                {% for item in data %}
                <div class="header-title-wrapper">
                    <div class="header-title-sno-list">{{  i }}</div>
                    <div class="header-title-list">{{ item.Atitle }}</div>
                    <div class="header-title-list">{{ item.Status }}</div>
                    <div class="header-title"></div>
                    <div class="header-title-action-list">
                        <div class="header-action-list"><a href="cmslistdelete/{{ item.id }}">Delete</a></div>
                        <div class="header-action-list"><a href="cmslistupdate/{{item.id}}">Update</a></div>
                        <div class="clearboth"></div>
                    </div>
                    <div class="clearboth"></div>
                </div>
                {% set i = i + 1 %}
                <div></div>
                {% endfor %}
            </div>
            <div class="cmsFormList">

            </div>
    </div>
{% endblock %}

