{% extends '::baseadmin.html.twig' %}


{% block body %}
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 ">
           <div class="addNew">
               <a href="{{ path('admin_settings')}}">ADD Email</a></div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="cmsFormList">
                <div class="header-title-wrapper_settings">
                    <div class="header-title-sno_settings">S.no</div>
                    <div class="header-title_settings">Email</div>
                    <div class="header-title_settings">Country</div>
                    <div class="header-title_settings">Form Type</div>
                    <div class="header-title_settings">Technical Type</div>
                    <div class="header-title_settings">Other Type</div>
                    <div class="header-title-action_settings">
                        Action
                    </div>
                    <div class="clearboth"></div>
                </div>
                {% set i = 1 %}
                {% for item in data %}
                <div class="header-title-wrapper_settings">
                    <div class="header-title-sno-list_settings">{{  i }}</div>
                    <div class="header-title-list_settings">{{ item.Email }}</div>
                    <div class="header-title-list_settings">{{ item.Country }}</div>
                    <div class="header-title-list_settings">{{ item.Type }}</div>
                    <div class="header-title-list_settings">{{ item.Technical }}</div>
                    <div class="header-title-list_settings">{{ item.Other }}</div>
                    <div class="header-title-action-list_settings">
                        <div class="header-action-list_settings"><a href="{{ app.request.getBaseURL()  }}/admin/settingsdelete/{{ item.Id }}" onclick="return delRecord({{ item.Id }})">Delete</a></div>
                        <div class="header-action-list_settings"><a href="{{ app.request.getBaseURL()  }}/admin/settingsupdate/{{ item.Id }}">Update</a></div>
                        <div class="clearboth"></div>
                    </div>
                    <div class="clearboth"></div>
                </div>
                {% set i = i + 1 %}
                <div></div>
                {% endfor %}
            </div>
            <div class="cmsFormList">

            </div>
    </div>

{% endblock %}

